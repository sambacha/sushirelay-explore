{
  "name": "sushiswap-relay-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "dev": "npx turbo run dev --parallel",
    "lint": "npx turbo run lint",
    "format": "npx prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2",
    "@ethersproject/shims": "^5.4.0",
    "@sushiswap/core": "1.4.2",
    "@sushiswap/sdk": "5.0.0-canary.7",
    "@uniswap/sdk-core": "^3.0.0",
    "@uniswap/v2-core": "1.0.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v2-sdk": "3.0.0-alpha.2",
    "@uniswap/v3-core": "1.0.0",
    "@uniswap/v3-periphery": "1.0.0",
    "@uniswap/v3-sdk": "3.0.0",
    "consola": "^2.15.3",
    "dotenv": "^8.2.0",
    "ethers": "^5.3.1",
    "express": "^4.17.2",
    "mongoose": "^5.11.15",
    "pm2": "^5.1.2",
    "serve": "^13.0.2",
    "socket.io": "^4.0.0"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  }
}
